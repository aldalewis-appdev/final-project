<h1>
  All users
</h1>


<hr>

<table class="table">
  <tr>
    <th>
      Username
    </th>
    
    <th>
      Email
    </th>

    <th>
      MBA Program
    </th>


    <th>
      Class
    </th>
    <th>
      User Details
    </th>

  </tr>

  <% @users.each do |user| %>
  <tr>
    <td>
      <%= user.username %>
    </td>

    <td>
    <%= user.email %>
    </td>

    <td>
      <%= user.school %>
    </td>

    <td>
      <%= user.grad_year %>
    </td>
    
    <td>
      <a href="/users/<%= user.id %>">Show Details</a>
    </td>

  </tr>
  <% end %>
</table>
